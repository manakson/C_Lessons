#include<stdio.h>
#include<stdlib.h>
int main()
{
float ms[5];
for (int i=0;i<5;i++)
{
printf("Please enter a float");
scanf("%f", &ms[i]);}
for (int i=0; i<5; i++)
printf("%f\n",ms[i]);
return 0;
}
