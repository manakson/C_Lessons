#include<stdio.h>
#include<stdlib.h>

int main()
{
char name[] = "manak";
printf("%c", name[2]);
return 0;
}
_______________________________

#include<stdio.h>
#include<stdlib.h>
int main()
{
char name[20];
printf("Please enter your name\n");
scanf("%s", &name);
printf("Your name is %s", name);
return 0;
}
