#include<stdio.h>
int main()
{char ms;
printf("\n Please enter your grade\t");
scanf("%c", &ms);
if (ms=='A')
{printf("Your grade is A");
}
else if (ms=='B')
{printf("Your grade is B");
}
else if (ms=='C')
{printf("Your grade is C");
}
else 
{printf("Fuck Yourself");
}
return 0;
 
}
